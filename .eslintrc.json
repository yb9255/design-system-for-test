{
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-unused-vars": 0,
        "@typescript-eslint/no-unused-vars": "warn",
        "no-shadow": 0,
        "@typescript-eslint/no-shadow": "off",
        "camelcase": "off",
        "react/require-default-props": 0,
        "lines-between-class-members": 0,
        "no-dupe-class-members": 0,
        "react/jsx-pascal-case": "off"
      }
    }
  ]
}
